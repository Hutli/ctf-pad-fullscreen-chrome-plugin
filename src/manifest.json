{
    "manifest_version": 2,
    "name": "Kalmarunionen Pad Fullscreen Plugin",
    "description": "To make etherpad fullscreen on kalmarunionen CTF-pad",
    "version": "0.0.1",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "js": [
                "update.js"
            ],
            "matches": [
                "https://pad.kalmarunionen.team/*"
            ]
        }
    ]
}